/*每个页面公共css */
.commonBgc {
  width: 100%;
  height: 20rpx;
  background-color: #f5f5f5;
}
.topTitle {
  text-align: center;
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 32rpx;
  font-family: FZCuYuan-M03S;
  font-weight: 500;
  color: #000;
  position: fixed;
  top: 0;
  left: 0;
  background-color: #fff;
  z-index: 999;
}
.topTitle .back {
  position: absolute;
  top: 0;
  left: 0;
  width: 120rpx;
  height: 88rpx;
}
.topTitle .back image {
  width: 17rpx;
  height: 32rpx;
}
.shareModel {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 999;
}
.shareModel .model {
  width: 395rpx;
  height: 239rpx;
  position: absolute;
  top: 0;
  right: 20rpx;
}
.shareModel .model image {
  width: 100%;
  height: 100%;
}
.shareModel .login {
  width: 580rpx;
  height: 620rpx;
  background: #FFFFFF;
  -webkit-border-radius: 30rpx;
          border-radius: 30rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  text-align: center;
}
.shareModel .login image {
  width: 495rpx;
  height: 253rpx;
  margin-top: 30rpx;
}
.shareModel .login view {
  text-align: center;
}
.shareModel .login .tip {
  margin: 40rpx 0 26rpx;
  font-size: 42rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: #333333;
}
.shareModel .login .tip1 {
  font-size: 30rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: #999;
}
.shareModel .login .btn {
  width: 366rpx;
  height: 81rpx;
  background: #F93A2D;
  -webkit-border-radius: 41rpx;
          border-radius: 41rpx;
  color: #fff;
  line-height: 81rpx;
  margin: auto;
  margin-top: 60rpx;
}

@font-face {font-family: "iconfont";
  src: url(data:application/vnd.ms-fontobject;base64,); /* IE9 */
  src: url(data:application/vnd.ms-fontobject;base64,#iefix) format('embedded-opentype'), 
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url(data:font/woff;base64,) format('woff'),
  url(data:font/ttf;base64,) format('truetype'), 
  url(../../static/img/iconfont.d211f6aa.svg#iconfont) format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

