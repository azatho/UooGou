<view><view class="none_img"><image src="../../static/noShops.png"></image></view><view style="background-color:#f5f5f5;width:100%;height:20rpx;"></view><view class="recommend"><view class="recommends"><view class="recommend_tit"><view class="line"></view><view class="title">商品推荐</view><view class="line1"></view></view></view><view class="common_goods"><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><block><view class="common_con"><view data-event-opts="{{[['tap',[['goShop',['$0'],[[['goodsList','',i,'supplier_id']]]]]]]}}" bindtap="__e"><image src="{{cdnUrl+item.goods_icon}}" mode></image><view class="name">{{''+item.goods_name+''}}</view><view class="discount"><block wx:for="{{item.coupon}}" wx:for-item="items" wx:for-index="k" wx:key="k"><view><view class="coupon">{{'领'+(items.man_cash/100-items.deduct_cash/100)+'元券'}}</view></view></block></view><view class="price">{{'￥'+item.goods_cost/100}}<text>{{item.goods_price/100}}</text></view></view><view data-event-opts="{{[['tap',[['goShop',['$0'],[[['goodsList','',i,'supplier_id']]]]]]]}}" class="shop" bindtap="__e"><view class="img"><image src="{{cdnUrl+item.supplier_icon}}" mode></image></view><view class="right_shop"><view class="title">{{''+item.supplier_name+''}}</view><block wx:if="{{item.base_price!=0}}"><view class="tip">{{'基础配送费：'+item.base_price/100+''}}</view></block><block wx:else><view class="tip">免配送费</view></block></view></view></view></block></block></view></view></view>