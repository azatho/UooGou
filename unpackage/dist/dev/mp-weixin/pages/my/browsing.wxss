
page {
	position: relative;
}
page image {
	width: 100%;
	height: 100%;
}
.flexd {
	position: fixed;
	left: 0;
	top: 0;
	width: 749rpx;
	height: 88rpx;
	background: #FFFFFF;
	-webkit-box-shadow: 0rpx 0rpx 5rpx 0rpx rgba(0, 0, 0, 0.16);
	        box-shadow: 0rpx 0rpx 5rpx 0rpx rgba(0, 0, 0, 0.16);
	line-height: 88rpx;
	padding: 0 30rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	font-family: PingFang SC;
	z-index: 111111;
}
.flexd .left {
	width: 17rpx;
	height: 32rpx;
}
.flexd .center {
	font-size: 36rpx;
	font-weight: bold;
	color: #333333;
}
.flexd .right {
	font-size: 26rpx;
	font-weight: 400;
	color: #FD484F;
}
.bgi {
	width: 374rpx;
	height: 314rpx;
	margin: 300rpx auto;
}

/* 浏览记录内容 */
.carry {
	padding: 30rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	font-size: 26rpx;
	font-family: PingFang SC;
	color: #333333;
}
.carry .time {
	font-size: 30rpx;
	font-weight: 500;
	margin-bottom: 20rpx;
}
.carry .record {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding-bottom: 10rpx;
	padding: 10rpx 0;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	border-bottom: 1rpx solid #F5F5F5;
}
.carry .record .checked {
	width: 36rpx;
	height: 36rpx;
	margin-right: 25rpx;
}
.carry .record .img {
	width: 120rpx;
	height: 120rpx;
	margin-right: 20rpx;
}
.carry .txt {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.carry .txt .tilte {
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}
.carry .two {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin-top: 20rpx;
}
.carry .two .money {
	color: #CF4F49;
	font-size: 26rpx;
}
.carry .two .gobuy {
	width: 100rpx;
	height: 40rpx;
	background: #FD484F;
	-webkit-border-radius: 20rpx;
	        border-radius: 20rpx;
	line-height: 40rpx;
	text-align: center;
	font-size: 22rpx;
	color: #FFFFFF;
}

/* 底部按钮 */
.btn {
	width: 750rpx;
	height: 100rpx;
	line-height: 100rpx;
	padding: 0 30rpx;
	padding-right: 0;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	position: fixed;
	left: 0;
	bottom: 0;
	background: #FFFFFF;
	-webkit-box-shadow: 0rpx 0rpx 2rpx 0rpx rgba(0, 0, 0, 0.3);
	        box-shadow: 0rpx 0rpx 2rpx 0rpx rgba(0, 0, 0, 0.3);
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	font-family: PingFang SC;
}
.btn image {
	width: 36rpx;
	height: 36rpx;
	margin-right: 10rpx;
	vertical-align: middle;
}
.btn .left {
	font-size: 26rpx;
	font-weight: 400;
	color: #333333;
}
.btn .right {
	width: 200rpx;
	height: 100rpx;
	background: #FD484F;
	font-size: 30rpx;
	font-weight: 500;
	color: #FFFFFF;
	line-height: 100rpx;
	text-align: center;
}

