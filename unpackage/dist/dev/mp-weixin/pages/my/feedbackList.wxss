page {
  width: 100%;
  padding-top: 20rpx;
  background-color: #F6F5F8;
}
.none {
  font-size: 26rpx;
  text-align: center;
  margin-top: 50rpx;
}
.panel {
  background-color: #fff;
  font-size: 26rpx;
}
.panel .title {
  padding: 15rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  border-bottom: 1rpx solid #F5F5F5;
}
.panel .title .color1 {
  color: #0055F2;
}
.panel .title .color2 {
  color: #F20000;
}
.panel .content {
  padding: 15rpx;
}
.panel .content text {
  font-family: Source Han Sans CN;
  font-weight: 300;
  color: #999999;
  color: #999;
}
.panel .content .detail {
  font-size: 26rpx;
  font-family: Source Han Sans CN;
  font-weight: 300;
  color: #999999;
  text-align: right;
  color: #999;
  margin-top: 15rpx;
}

