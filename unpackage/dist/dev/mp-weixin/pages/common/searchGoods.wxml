<view><view class="status_bar"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="left_img" bindtap="__e"><image src="../../static/leftback.png" mode></image></view><view class="input"><input type="text" placeholder="请输入关键字" data-event-opts="{{[['input',[['__set_model',['','value','$event',[]]]]]]}}" value="{{value}}" bindinput="__e"/><image class="search_img" src="../../static/search(1).png" data-event-opts="{{[['tap',[['search',['$event']]]]]}}" bindtap="__e"></image><view data-event-opts="{{[['tap',[['del',['$event']]]]]}}" class="del" bindtap="__e"><image src="../../static/del.png"></image></view></view><view data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="searchWord" bindtap="__e">搜索</view></view><block wx:if="{{list.length!=0}}"><view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="i" wx:key="i"><block><view data-event-opts="{{[['tap',[['goGoodsDetail',['$0','$1','$2'],[[['list','',i,'goods_index']],[['list','',i,'activity_id']],[['list','',i,'activity_type']]]]]]]}}" class="goods_msg" bindtap="__e"><view class="good_img"><image src="{{cdnUrl+item.goods_icon}}" mode></image></view><view class="right"><view class="title">{{''+item.goods_name+''}}</view><view class="tip"><block wx:if="{{item.goods_sale!=0}}"><text>{{"好评"+item.goods_sale+"%"}}</text></block><block wx:else><text>暂无评分</text></block></view><view class="discount"><block wx:for="{{item.coupons}}" wx:for-item="items" wx:for-index="k" wx:key="k"><block><view class="{{[items.user_hav==1?'coupon1':'coupon']}}">{{'领'+items.deduct_cash/100+'元券'}}</view></block></block></view><view class="price">{{'￥'+item.goods_cost/100}}<text>{{"￥"+item.goods_price/100}}</text></view></view></view></block></block></view></block><block wx:else><view class="noneImg"><image src="../../static/zwsj.png" mode></image></view></block></view>