.noneImg {
  margin: 200rpx 0 0;
  text-align: center;
}
.noneImg image {
  width: 640rpx;
  height: 480rpx;
}
.status_bar {
  padding: 15rpx 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.status_bar .left_img {
  width: 50rpx;
  height: 70rpx;
}
.status_bar .left_img image {
  margin-top: 19rpx;
  width: 18rpx;
  height: 32rpx;
}
.status_bar .input {
  width: 410rpx;
  height: 70rpx;
  padding: 0 70rpx 0 60rpx;
  line-height: 70rpx;
  height: 70rpx;
  background: #eef1f4;
  -webkit-border-radius: 35rpx;
          border-radius: 35rpx;
  position: relative;
}
.status_bar .input input {
  height: 70rpx !important;
  line-height: 70rpx !important;
}
.status_bar .input .search_img {
  width: 30rpx;
  height: 28rpx;
  position: absolute;
  top: 20rpx;
  left: 25rpx;
}
.status_bar .input .del {
  width: 80rpx;
  height: 70rpx;
  position: absolute;
  top: 0;
  right: 0;
}
.status_bar .input .del image {
  width: 30rpx;
  height: 30rpx;
  margin: 20rpx  0 0 25rpx;
}
.status_bar .searchWord {
  height: 70rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
  line-height: 70rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333333;
}
.goods_msg {
  padding: 40rpx 30rpx 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.goods_msg .good_img image {
  width: 160rpx;
  height: 160rpx;
  background: #F5F5F5;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  margin-right: 20rpx;
}
.goods_msg .right .title {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: wrap;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.goods_msg .right .tip {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #999999;
  margin: 8rpx 0 15rpx;
}
.goods_msg .right .tip text {
  margin-right: 30rpx;
}
.goods_msg .right .discount {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.goods_msg .right .discount view {
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  position: relative;
  font-size: 16rpx;
  font-family: PingFang SC;
  font-weight: 400;
  margin-bottom: 10rpx;
  margin-right: 10rpx;
}
.goods_msg .right .discount .coupon {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  color: #fff;
}
.goods_msg .right .discount .coupon1 {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  color: #ff3b32;
}
.goods_msg .right .price {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #ff3f3f;
}
.goods_msg .right .price text {
  margin-left: 20rpx;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #999999;
  text-decoration: line-through;
}
.shop_msg {
  width: 690rpx;
  margin: auto;
  padding: 0 0 0 180rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  border-bottom: 2rpx solid #f5f5f5;
  height: 66rpx;
  line-height: 66rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #999999;
}
.shop_msg image {
  width: 29rpx;
  height: 29rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.shop_msg text {
  margin-left: 20rpx;
}

