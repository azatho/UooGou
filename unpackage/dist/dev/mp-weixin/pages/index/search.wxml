<view><view class="status_bar"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="left_img" bindtap="__e"><image src="../../static/leftback.png" mode></image></view><view class="input"><input type="text" placeholder="请输入商品名称" data-event-opts="{{[['input',[['__set_model',['','value','$event',[]]]]]]}}" value="{{value}}" bindinput="__e"/><image class="search_img" src="../../static/search(1).png" data-event-opts="{{[['tap',[['search',['$event']]]]]}}" bindtap="__e"></image><view data-event-opts="{{[['tap',[['del',['$event']]]]]}}" class="del" bindtap="__e"><image src="../../static/del.png"></image></view></view><view data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="searchWord" bindtap="__e">搜索</view></view><block wx:if="{{history_list.length!=0}}"><view><view class="title"><view class="word">历史搜索</view><view data-event-opts="{{[['tap',[['empty',['$event']]]]]}}" class="del" bindtap="__e"><image src="../../static/del1.png" mode></image></view></view><view class="history_list"><block wx:for="{{history_list}}" wx:for-item="item" wx:for-index="j" wx:key="j"><block><block wx:if="{{item}}"><view data-event-opts="{{[['tap',[['jump',['$0'],[[['history_list','',j]]]]]]]}}" class="history" bindtap="__e">{{item}}</view></block></block></block></view></view></block><block wx:else><view class="none_history">暂无搜索历史</view></block></view>