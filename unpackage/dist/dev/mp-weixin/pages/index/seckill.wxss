.seckill {
  padding-top: 180rpx;
}
.seckill .top {
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: #F72D21;
  z-index: 333;
}
.seckill .top .tabbar {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  position: relative;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
}
.seckill .top .tabbar .back {
  width: 100rpx;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
}
.seckill .top .tabbar .back image {
  width: 18px;
  height: 18px;
}
.seckill .top .tabs {
  padding: 0 30rpx;
  height: 90rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.seckill .top .tabs .tab {
  width: 50%;
  line-height: 60rpx;
  text-align: center;
  font-family: PingFang SC;
  color: #FFFFFF;
  position: relative;
}
.seckill .top .tabs .tab .txt1 {
  font-weight: 400;
  font-size: 26rpx;
}
.seckill .top .tabs .tab .txt2 {
  font-weight: bold;
  font-size: 30rpx;
}
.seckill .top .tabs .tab .line {
  width: 54rpx;
  height: 3rpx;
  background: #FFFFFF;
  -webkit-border-radius: 2rpx;
          border-radius: 2rpx;
  position: absolute;
  bottom: 30rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.seckill .bgc {
  width: 100%;
  height: 215rpx;
  background: -webkit-gradient(linear, left bottom, left top, from(#FF5F53), to(#F6281B));
  background: -webkit-linear-gradient(bottom, #FF5F53, #F6281B);
  background: linear-gradient(0deg, #FF5F53, #F6281B);
  position: fixed;
  top: 180rpx;
  left: 0;
  z-index: -1;
}
.seckill .goods_con {
