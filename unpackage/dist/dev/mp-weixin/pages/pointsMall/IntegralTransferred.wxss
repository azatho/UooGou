.page {
  height: 100vh;
  background-color: #F5F5F5;
}
.number {
  height: 220rpx;
  background: #FFFFFF;
  padding: 20rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.number .itemTile {
  height: 120rpx;
  line-height: 120rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 600;
  color: #333333;
}
.number .itemNum {
  height: 100rpx;
}
.tel {
  margin-top: 20rpx;
  height: 90rpx;
  background-color: white;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.tel .telText {
  width: 150rpx;
  text-align: center;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333333;
}
.vipInfor {
  padding: 10rpx 25rpx;
}
.vipInfor .vipHead {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333333;
}
.vipInfor .vipItem {
  margin-top: 10rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.vipInfor .vipItem .vipImg {
  width: 80rpx;
  height: 80rpx;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  overflow: hidden;
}
.vipInfor .vipItem .vipImg image {
  width: 100%;
  height: 100%;
}
.vipInfor .vipItem .vipText {
  padding-left: 15rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #999999;
}
.none {
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 15rpx;
  color: #666666;
}
.btn {
  margin-top: 40rpx;
  width: 90%;
  background: #F4483C;
  font-size: 30rpx;
  -webkit-border-radius: 45rpx;
          border-radius: 45rpx;
}

